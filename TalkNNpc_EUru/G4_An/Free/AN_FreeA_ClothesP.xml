	<message label="001_02" attrib="00 00 00 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" />Эт<tag a="50" b="24" arg="0x17 0xcd00 0x4 0x43e 0x442 0x2 0x430 0x2 0x43e 0x2 0x438" /> <tag a="125" b="5" arg="0xcd00" />
