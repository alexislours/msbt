	<message label="104" attrib="00 00 00 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" />Бывай, <tag a="110" b="3" />!</message>
	<message label="105" attrib="00 00 00 00 00 00 00 00"><anim id="Grin" target="actor" targetSpec="" />Мне кажется, я тебя узнала
