	<message label="003_a" attrib="00 00 00 00 00 00 00 00">Измени.</message>
	<message label="003_b" attrib="00 00 00 00 00 00 00 00">Не меняй ничего.</message>
	<message label="004" attrib="00 00 02 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" /><10_5 />Обалдеть<endTag a="10" b="5" />, <tag a="110" b="3" />!<delay8 />
