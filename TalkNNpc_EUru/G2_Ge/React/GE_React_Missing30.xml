	<message label="004_01" attrib="00 00 00 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" /><10_5 />С ума сойти<endTag a="10" b="5" /><delay8 />! <tag a="50" b="3" /><tag a="110" b="3" />!<delay8 />
