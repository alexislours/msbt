	<message label="003" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="003_01" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />С возвращением, <anim id="BigSmile" target="actor" targetSpec="" /><tag a="110" b="3" />!
