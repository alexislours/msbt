	<message label="004_03" attrib="00 00 00 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" /><tag a="50" b="3" /><tag a="110" b="3" />!<delay40 /> <anim id="Confused" target="actor" targetSpec="" />Давненько тебя
