	<message label="003" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="003_01" attrib="00 00 00 00 00 00 00 00"><anim id="QuestionMark" target="actor" targetSpec="" />Дорогуша,<delay8 /> ты осознанно
