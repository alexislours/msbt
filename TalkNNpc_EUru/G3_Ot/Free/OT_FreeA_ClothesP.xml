	<message label="004" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="004_01" attrib="00 00 00 00 00 00 00 00"><anim id="FaintSmile" target="actor" targetSpec="" /><tag a="50" b="3" /><tag a="125" b="6" arg="0xcd02" /> на тебе<delay8 />
