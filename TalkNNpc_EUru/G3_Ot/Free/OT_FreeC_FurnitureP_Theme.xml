	<message label="010" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><20_2 arg="9" /><tag a="50" b="3" /><tag a="125" b="0" arg="0x0" /> <anim id="Love" target="actor" targetSpec="" />до
