	<message label="001_a" attrib="00 00 00 00 00 00 00 00">По рукам!</message>
	<message label="001_b" attrib="00 00 00 00 00 00 00 00">Нет, не сейчас<tag a="110" b="0" /></message>
	<message label="002" attrib="00 00 01 00 00 00 00 00"><anim id="BigSmile" target="actor" targetSpec="" />Итак! <10_4 /><anim id="Thinking" target="actor" targetSpec="" />Для начала вспомним
