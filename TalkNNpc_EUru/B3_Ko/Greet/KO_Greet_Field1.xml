<msbt attribsize="8">
	<message label="001" attrib="00 00 0c 01 00 07 00 00"><anim id="Greeting" target="actor" targetSpec="" />Доброе утро!<delay8 /><anim id="FaintSmile" target="actor" targetSpec="" /> Лучшее время для
