<msbt attribsize="8">
	<message label="001" attrib="00 00 0c 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />Да это же <tag a="110" b="3" />!<anim id="EmotEnd" target="actor" targetSpec="" />
