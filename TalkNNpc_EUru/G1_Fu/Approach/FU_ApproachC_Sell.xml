	<message label="403_a" attrib="00 00 00 00 00 00 00 00">Продано!</message>
	<message label="403_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />А еще дешевле?</message>
	<message label="404" attrib="00 00 02 00 00 00 00 00"><anim id="Thinking" target="actor" targetSpec="" />Так-так<tag a="110" b="0" /> Посмотрим<tag a="110" b="0" />
<anim id="QuestionMark" target="actor" targetSpec="" />Как насчет <tag a="90" b="21" arg="0x1" />?</message>
	<message label="404_a" attrib="00 00 00 00 00 00 00 00">Беру!</message>
	<message label="404_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />А еще дешевле?</message>
	<message label="405" attrib="00 00 02 00 00 00 00 00"><anim id="Nodding" target="actor" targetSpec="" />Ну хорошо.<anim id="EmotEnd" target="actor" targetSpec="" /> Мое последнее<delay8 /><anim id="Serious" target="actor" targetSpec="" />
