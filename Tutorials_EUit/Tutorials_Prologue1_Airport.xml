	<message label="2002_01_b" attrib="00 00 00">Ho cambiato idea.</message>
	<message label="2004" attrib="02 00 00"><anim id="BigSmile" target="mainNpc" targetSpec="" /><anim id="BigSmile" target="subNpc" targetSpec="" />Ho trovato <tag a="115" b="0" arg="0xcd00" /><delay8 /> da
<tag a="115" b="2" arg="0xcd00" />!<delay frames="40" /> <anim id="Hello" target="mainNpc" targetSpec="" /><anim id="Hello" target="subNpc" targetSpec="" />È giusto?</message>
