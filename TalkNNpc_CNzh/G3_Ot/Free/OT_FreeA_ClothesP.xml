	<message label="016" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="016_01" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />那件<tag a="125" b="5" arg="0xcd00" />，<delay8 />
