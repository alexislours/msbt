	<message label="018" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="018_01" attrib="00 00 00 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" /><tag a="125" b="5" arg="0xcd00" />吗～<delay20 />
