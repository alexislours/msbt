<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="001_01" attrib="00 00 00 00 00 00 00 00"><anim id="FaintSmile" target="actor" targetSpec="" />你给我的这件<anim id="EmotEnd" target="actor" targetSpec="" /><delay8 />
