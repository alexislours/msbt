<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="001_01" attrib="00 00 00 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" />噢，<delay8 /><tag a="110" b="3" />！<delay20 />
