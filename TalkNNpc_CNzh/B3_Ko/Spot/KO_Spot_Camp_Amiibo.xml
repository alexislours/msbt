<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 13 00"><tag a="60" b="20" /></message>
	<message label="001_01" attrib="00 00 00 00 00 00 13 00"><tag a="100" b="5" /><colour id="1" />哟，我是<tag a="115" b="1" arg="0xcd00" />！<delay frames="40" />
