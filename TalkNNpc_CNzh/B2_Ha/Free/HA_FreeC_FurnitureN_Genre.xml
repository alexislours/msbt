	<message label="001" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><anim id="HappyDance" target="actor" targetSpec="" /><tag a="125" b="0" arg="0x0" />，<20_2 arg="9" /><delay20 />
