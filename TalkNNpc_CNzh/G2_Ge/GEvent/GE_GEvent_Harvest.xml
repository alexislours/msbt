	<message label="1041" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="1041_01" attrib="00 00 00 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" />谢谢你，<delay8 /><tag a="110" b="4" />！<delay40 />
