	<message label="012" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="1" arg="0xd 0xe 0xf" /></message>
	<message label="013" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />哇～！<delay40 />
