	<message label="001" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><20_2 arg="9" /><anim id="FaintSmile" target="actor" targetSpec="" />这<tag a="125" b="0" arg="0x0" /><delay8 />
