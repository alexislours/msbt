	<message label="021" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><20_2 arg="9" />有<tag a="125" b="0" arg="0x0" />在<tag a="110" b="0" /><pageBreak /><20_3 arg="6" /><20_3 arg="3" /><anim id="Thinking" target="actor" targetSpec="" /><tag a="125" b="0" arg="0x0" /><delay8 />
