	<message label="100" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="100_01" attrib="00 00 00 00 00 00 00 00">你喜欢<anim id="Hello" target="actor" targetSpec="" />观赏艺术品吗？<delay frames="40" />
