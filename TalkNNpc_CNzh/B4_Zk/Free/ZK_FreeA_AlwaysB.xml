	<message label="028" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="12" arg="0x1e 0x1d" /></message>
	<message label="029" attrib="00 00 00 00 00 00 00 00"><tag a="110" b="0" /><anim id="Shocked" target="actor" targetSpec="" />啊！好险！<delay frames="40" />
