	<message label="013" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="013_01" attrib="00 00 00 00 00 00 00 00"><anim id="Serious" target="actor" targetSpec="" />终于到这一天了，<delay frames="40" />
