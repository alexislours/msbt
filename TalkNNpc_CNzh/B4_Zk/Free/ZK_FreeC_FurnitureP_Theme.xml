	<message label="020" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><20_2 arg="9" /><anim id="Serious" target="actor" targetSpec="" /><tag a="125" b="0" arg="0x0" />啊～<delay frames="40" />
