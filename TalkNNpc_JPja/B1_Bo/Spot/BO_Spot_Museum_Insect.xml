<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="001_01" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />こうやって　ムシさんを<ruby a="2" b="2">み</ruby>見てると<delay20 />
