<msbt attribsize="4">
	<message label="001" attrib="00 00 00 00">Tuurlijk!</message>
	<message label="002" attrib="00 00 00 00">Ik heb het te druk<tag a="110" b="0" /></message>
	<message label="003" attrib="00 00 00 00">Ik ben een en al oor!</message>
	<message label="004" attrib="00 00 00 00">Ik heb nu wel even tijd!</message>
	<message label="005" attrib="00 00 00 00"><anim id="EmotEnd" target="player" targetSpec="" />Een ander keertje<tag a="110" b="0" /></message>
	<message label="006" attrib="00 00 00 00"><anim id="Assent" target="player" targetSpec="" />Oké!</message>
	<message label="007" attrib="00 00 00 00">Natuurlijk help ik je.</message>
	<message label="008" attrib="00 00 00 00">Over die winkel...</message>
	<message label="009" attrib="00 00 00 00">Ik heb materialen!</message>
	<message label="010" attrib="00 00 00 00">Wat heb je nodig?</message>
	<message label="011" attrib="00 00 00 00">Ah, oké.</message>
	<message label="012" attrib="00 00 00 00">Hier, alsjeblieft!</message>
	<message label="013" attrib="00 00 00 00">Misschien later...</message>
	<message label="014_01" attrib="00 00 00 00">1 (<colour id="1" /> <tag a="90" b="12" arg="0x0" /><colour id="65535" />)</message>
	<message label="014_02" attrib="00 00 00 00">5 (<colour id="1" /> <tag a="90" b="13" arg="0x1" /><colour id="65535" />)</message>
	<message label="014_03" attrib="00 00 00 00">Ik heb me bedacht.</message>
	<message label="015" attrib="00 00 00 00">Wat is dit?</message>
	<message label="016" attrib="00 00 00 00">Wat is de dagtopper?</message>
	<message label="017_01" attrib="00 00 00 00">Prima. Ga je gang.</message>
	<message label="017_02" attrib="00 00 00 00">Eh, nee. Eén seconde.</message>
	<message label="018_01" attrib="00 00 00 00">Ruim maar op, alsjeblieft!</message>
	<message label="018_02" attrib="00 00 00 00">Ik hou het wel bij me.</message>
</msbt>