<msbt attribsize="8">
	<message label="001" attrib="00 00 0c 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />Ah,<delay8 /> ¡pero si es <gender2 m="el" f="la" /> nunca bien
