	<message label="011" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="011_01" attrib="00 00 00 00 00 00 00 00"><anim id="Worried" target="actor" targetSpec="" />Ay,<delay8 /> qué vergüenza<tag a="110" b="0" /><delay20 /> <anim id="Sleepy" target="actor" targetSpec="" />Ayer me acosté
