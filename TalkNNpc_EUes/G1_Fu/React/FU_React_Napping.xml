	<message label="014" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="014_01" attrib="00 00 00 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />Carambola,<delay8 /> me he quedado
