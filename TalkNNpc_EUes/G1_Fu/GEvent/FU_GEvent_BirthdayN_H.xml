	<message label="061_a" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />Falta el mío, toma</message>
	<message label="061_b" attrib="00 00 00 00 00 00 00 00">Yo no he traído...</message>
	<message label="062" attrib="00 00 02 00 00 00 00 00"><anim id="FaintSmile" target="actor" targetSpec="" />Me han dicho por aquí que se te
