	<message label="005" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="005_01" attrib="00 00 00 00 00 00 00 00"><anim id="HappyDance" target="actor" targetSpec="" />Je, je, je<tag a="110" b="0" /><delay20 /> Ya llevaba una
