	<message label="105" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="105_01" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />Je, je, je<tag a="110" b="0" /><delay frames="16" /> Ya llevo una buena
