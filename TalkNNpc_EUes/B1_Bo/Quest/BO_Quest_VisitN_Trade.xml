	<message label="012" attrib="00 00 00 00 00 00 00 00"><anim id="Thinking" target="actor" targetSpec="" />¿Nada de nada?<delay20 /> <anim id="Restless" target="actor" targetSpec="" />Bueno<tag a="110" b="0" /><delay8 /> <anim id="FaintSmile" target="actor" targetSpec="" />yo había
