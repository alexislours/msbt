	<message label="101_a" attrib="00 00 00 00 00 00 00 00">¡Estupendo!</message>
	<message label="101_b" attrib="00 00 00 00 00 00 00 00">Mejor no</message>
	<message label="102" attrib="00 00 02 00 00 00 00 00"><anim id="BigSmile" target="actor" targetSpec="" />¡<tag a="50" b="3" /><tag a="50" b="1" /><tag a="125" b="0" arg="0x0" />!<delay8 />
<anim id="Hesitate" target="actor" targetSpec="" />A mí también me encanta<tag a="110" b="0" /><pageBreak /><anim id="Smiling" target="actor" targetSpec="" />¿<tag a="90" b="21" arg="0x0" /> te parece
