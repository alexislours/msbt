	<message label="102_a" attrib="00 00 00 00 00 00 00 00">¡Me lo parece!</message>
	<message label="102_b" attrib="00 00 00 00 00 00 00 00">No me lo parece</message>
	<message label="103" attrib="00 00 00 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />¿Has cambiado de idea?<delay8 />
<anim id="Negative" target="actor" targetSpec="" />Pues qué pena, <tag a="110" b="6" /><tag a="110" b="0" /></message>
	<message label="104" attrib="00 00 00 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" />Oye, que no tienes bayas
