	<message label="100_a" attrib="00 00 00 00 00 00 00 00">¡Trato hecho!</message>
	<message label="100_b" attrib="00 00 00 00 00 00 00 00">Mejor no</message>
	<message label="101" attrib="00 00 02 00 00 00 00 00"><anim id="IdeaBulb" target="actor" targetSpec="" />¿<tag a="50" b="3" /><tag a="50" b="1" /><tag a="125" b="0" arg="0x0" />?<delay8 />
