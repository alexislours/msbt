	<message label="104" attrib="00 00 00 00 00 00 00 00"><anim id="Worried" target="actor" targetSpec="" />Huy, vale<tag a="110" b="0" /><delay frames="20" /> <anim id="EmotEnd" target="actor" targetSpec="" />Perdona por haberte
