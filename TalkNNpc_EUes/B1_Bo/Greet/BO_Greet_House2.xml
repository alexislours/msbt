	<message label="003" attrib="00 00 0c 01 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />Vaya, <tag a="110" b="3" />.<delay20 /> <anim id="FaintSmile" target="actor" targetSpec="" />A estas
