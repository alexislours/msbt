	<message label="001" attrib="00 00 00 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" /><10_5 />¡Qué<endTag a="10" b="5" /><delay8 /> <10_5 />fuer<endTag a="10" b="5" /><delay8 /><10_5 />te!<endTag a="10" b="5" />
<anim id="BigSmile" target="subNpc" targetSpec="" />¡<tag a="50" b="3" /><tag a="110" b="31" arg="0xcd01" />!<delay20 />
