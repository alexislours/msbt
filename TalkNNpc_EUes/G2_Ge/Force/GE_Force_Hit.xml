<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 00 00"><anim id="Surprised" target="actor" targetSpec="" />¡Ay!<delay20 /> ¿<tag a="50" b="3" /><tag a="110" b="3" />?<delay20 />
<anim id="Worried" target="actor" targetSpec="" />¿Pero a qué viene esta hostilidad?<delay20 />
<anim id="Confused" target="actor" targetSpec="" />Si te molesta algo, ¡dímelo!</message>
	<message label="002" attrib="00 00 00 00 00 00 00 00"><anim id="TakenAback" target="actor" targetSpec="" />¡Aaay! <delay20 />¡Que me ha dolido!<delay20 />
<anim id="Angry" target="actor" targetSpec="" />¡Como me hayas estropeado
