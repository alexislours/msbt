	<message label="003" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />¿De veras?<delay frames="40" /> <anim id="HappyFlower" target="actor" targetSpec="" />¡Fabuloso!<delay20 />
