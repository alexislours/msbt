	<message label="002_02" attrib="00 00 00 00 00 00 00 00"><anim id="HappyDance" target="actor" targetSpec="" />¡Es solo un pequeño detallito!</message>
	<message label="002_03" attrib="00 00 00 00 00 00 00 00"><anim id="Nodding" target="actor" targetSpec="" />¿A que tengo buen gusto, eh?</message>
