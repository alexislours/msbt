	<message label="002" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="002_01" attrib="00 00 00 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" />헬로!<delay20 />
