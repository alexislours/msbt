	<message label="200" attrib="00 00 00 00 00 00 00 00"><tag a="100" b="0" /><anim id="Angry" target="actor" targetSpec="" /><tag a="110" b="4" /><gender2 m="군" f="양" /><tag a="110" b="0" /><delay20 />
