	<message label="203" attrib="00 00 02 00 00 00 00 00"><anim id="Worried" target="actor" targetSpec="" />있잖아<tag a="110" b="0" /><delay20 /> <tag a="110" b="4" /><gender2 m="군" f="양" />!<delay frames="40" />
