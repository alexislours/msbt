	<message label="301" attrib="00 00 00 00 00 00 13 00"><tag a="60" b="20" /></message>
	<message label="301_01" attrib="00 00 00 00 00 00 13 00">여보세요, <delay8 /><tag a="110" b="10" /><gender5 m="이" f="" />다!<delay frames="40" />
