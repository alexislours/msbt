	<message label="005" attrib="00 00 00 00 00 00 00 00">다음!<pageBreak /><tag a="10" b="12" /><size value="150" />포즈 <anim id="Greeting" target="actor" targetSpec="" /><delay20 />넘버 2!<size value="100" /></message>
	<message label="006" attrib="00 00 00 00 00 00 00 00"><anim id="Dance" target="actor" targetSpec="" /><tag a="10" b="12" /><size value="150" />하앗!<size value="100" /></message>
