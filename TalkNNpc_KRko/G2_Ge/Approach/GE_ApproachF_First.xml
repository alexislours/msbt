	<message label="007" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="007_01" attrib="00 00 00 00 00 00 00 00"><anim id="HappyDance" target="actor" targetSpec="" />그럼 잘 부탁해!<delay frames="40" />
