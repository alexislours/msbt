	<message label="006" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="006_01" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />그 <tag a="125" b="0" arg="0x0" /><delay20 />
