	<message label="030" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><20_2 arg="9" /><anim id="Eh" target="actor" targetSpec="" /><tag a="125" b="0" arg="0x0" /><gender5 m="이" f="" />요?<delay frames="40" />
