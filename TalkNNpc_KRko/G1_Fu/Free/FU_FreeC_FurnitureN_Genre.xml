	<message label="021" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><20_2 arg="9" /><anim id="FaintSmile" target="actor" targetSpec="" /><tag a="125" b="0" arg="0x0" /><gender5 m="은" f="는" /><delay20 />
