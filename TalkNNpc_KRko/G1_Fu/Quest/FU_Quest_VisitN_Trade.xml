	<message label="006_a" attrib="00 00 00 00 00 00 00 00">있어!</message>
	<message label="006_b" attrib="00 00 00 00 00 00 00 00">딱히 없네</message>
	<message label="007" attrib="00 00 00 00 00 00 00 00"><anim id="QuestionMark" target="actor" targetSpec="" />어떤 거예요?<delay frames="40" />
