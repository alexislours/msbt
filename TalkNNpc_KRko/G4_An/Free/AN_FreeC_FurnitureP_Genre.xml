	<message label="016" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><anim id="Thinking" target="actor" targetSpec="" /><tag a="125" b="0" arg="0x0" /><gender5 m="이" f="가" /><20_2 arg="9" />
