	<message label="012" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><anim id="Aha" target="actor" targetSpec="" /><tag a="125" b="0" arg="0x0" /><gender5 m="이" f="가" /><delay8 /><20_2 arg="9" />
