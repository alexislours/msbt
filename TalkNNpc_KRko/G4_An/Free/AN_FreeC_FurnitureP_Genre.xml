	<message label="010" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><anim id="Shocked" target="actor" targetSpec="" /><tag a="125" b="0" arg="0x0" /><gender5 m="을" f="를" /><delay20 />
