<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 00 00">그러고 보니 <delay8 /><anim id="QuestionMark" target="actor" targetSpec="" />오늘 <colour id="3" />죠니<colour id="65535" /> 봤냐?<delay frames="40" />
