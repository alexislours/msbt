<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 00 00"><anim id="FaintSmile" target="actor" targetSpec="" />있지, <delay20 /><anim id="AbsentMindedness" target="actor" targetSpec="" />지금 여기에
