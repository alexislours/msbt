	<message label="022_a" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="player" targetSpec="" />자, 받아!</message>
	<message label="022_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="player" targetSpec="" />고마워～</message>
	<message label="023" attrib="00 00 02 00 00 00 00 00"><anim id="HappyDance" target="actor" targetSpec="" /><tag a="110" b="4" />,<delay20 />
