<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="001_01" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />지금 입은 옷<delay20 />
<tag a="110" b="4" />한테<delay8 />
