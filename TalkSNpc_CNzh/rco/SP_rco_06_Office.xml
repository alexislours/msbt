	<message label="201_a" attrib="00 00 00 00">我会去找找！</message>
	<message label="201_b" attrib="00 00 00 00">还是下次吧</message>
	<message label="202" attrib="00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />明白狸！<10_4 />
