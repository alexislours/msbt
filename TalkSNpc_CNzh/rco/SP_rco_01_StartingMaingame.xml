	<message label="008_b" attrib="00 00 00 00"><anim id="Negative" target="player" targetSpec="" />不支付</message>
	<message label="008_c" attrib="00 00 00 00"><anim id="OhGeez" target="player" targetSpec="" />我没带哦！</message>
