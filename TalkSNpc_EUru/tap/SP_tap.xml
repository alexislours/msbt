	<message label="002_a" attrib="00 00 00 00">Хочу видеть сон.</message>
	<message label="002_b" attrib="00 00 00 00">Хочу поделиться сном.</message>
	<message label="002_c" attrib="00 00 00 00">Где я?</message>
	<message label="002_d" attrib="00 00 00 00">Хочу проснуться.</message>
