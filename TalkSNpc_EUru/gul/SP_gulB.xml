	<message label="103_b" attrib="00 00 00 00"><anim id="Negative" target="actor" targetSpec="" />Ни за что!</message>
	<message label="105" attrib="00 00 00 00"><anim id="OhGeez" target="actor" targetSpec="" />Я тут решил мозгами пораскинуть.<delay frames="40" />
