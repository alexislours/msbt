	<message label="002_04_e" attrib="00 00 00 00"><anim id="Negative" target="actor" targetSpec="" />Ничего не надо.</message>
	<message label="002_05" attrib="06 00 00 00"><anim id="QuestionMark" target="actor" targetSpec="" />Я могу еще чем-то помочь?</message>
	<message label="002_05_a" attrib="00 00 00 00"><anim id="Explain" target="actor" targetSpec="" />Мелодия острова...</message>
	<message label="002_05_b" attrib="00 00 00 00"><anim id="Explain" target="actor" targetSpec="" />Флаг острова...</message>
