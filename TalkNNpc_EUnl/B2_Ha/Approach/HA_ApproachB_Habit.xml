<msbt attribsize="8">
	<message label="001" attrib="00 00 02 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" /><tag a="50" b="3" /><tag a="110" b="3" />!<delay40 /> <anim id="HappyDance" target="actor" targetSpec="" /><tag a="50" b="3" /><tag a="110" b="6" />!<pageBreak /><anim id="Oops" target="actor" targetSpec="" />Verdraaid<tag a="110" b="0" /> Ik zei voor de zoveelste
