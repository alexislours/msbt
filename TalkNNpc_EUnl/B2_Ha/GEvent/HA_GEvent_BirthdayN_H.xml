	<message label="001" attrib="00 00 00 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" /><tag a="50" b="3" /><tag a="110" b="3" />!<delay frames="30" /> <anim id="Smiling" target="actor" targetSpec="" />Je bent gekomen!</message>
