	<message label="201_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />Hmmmmmm...</message>
	<message label="202" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />Eenmaal, andermaal<tag a="110" b="0" /> <delay frames="35" /><anim id="Smiling" target="actor" targetSpec="" />Verkocht aan
