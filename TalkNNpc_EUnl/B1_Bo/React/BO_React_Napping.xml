	<message label="013" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="013_01" attrib="00 00 00 00 00 00 00 00">Hmm<tag a="110" b="0" /> hè?<delay frames="40" /> <anim id="QuestionMark" target="actor" targetSpec="" />Wacht, ben ik nou in
