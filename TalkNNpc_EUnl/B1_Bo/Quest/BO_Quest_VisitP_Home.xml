	<message label="012" attrib="00 00 00 00 00 00 00 00"><anim id="BigSmile" target="actor" targetSpec="" />Hallo, daar ben ik dan!<delay frames="40" /> <anim id="Smiling" target="actor" targetSpec="" />Helemaal
