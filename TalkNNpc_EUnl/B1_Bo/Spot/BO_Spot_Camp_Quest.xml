	<message label="208_02" attrib="00 00 00 00 00 00 00 00"><anim id="Clapping" target="actor" targetSpec="" />Kom op!<delay8 /> Ik ben echt superbenieuwd!</message>
	<message label="209" attrib="00 00 00 00 00 00 00 00"><anim id="BigSmile" target="actor" targetSpec="" />Bedankt voor het souvenir!<delay frames="40" /><anim id="Blushing" target="actor" targetSpec="" /> Dit
