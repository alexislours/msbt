<msbt attribsize="8">
	<message label="101" attrib="00 00 0c 01 00 00 00 00">Nou,<delay20 /><anim id="FaintSmile" target="actor" targetSpec="" /> wat is er aan de hand,<delay20 /> <tag a="110" b="5" />?<setNpcTalkChoices chatOption="0" cancelOption="0" /></message>
