	<message label="103" attrib="00 00 00 00 00 00 00 00"><anim id="Assent" target="actor" targetSpec="" />O, oké<tag a="110" b="0" /></message>
	<message label="104" attrib="00 00 00 00 00 00 00 00"><anim id="Worried" target="actor" targetSpec="" />Sorry, maar<tag a="110" b="0" /><delay20 /> <anim id="Negative" target="actor" targetSpec="" />volgens mij heb je niet
