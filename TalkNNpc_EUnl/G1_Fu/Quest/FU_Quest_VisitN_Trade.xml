	<message label="006_b" attrib="00 00 00 00 00 00 00 00">Nee, hoor. Dank je.</message>
	<message label="007" attrib="00 00 00 00 00 00 00 00"><anim id="QuestionMark" target="actor" targetSpec="" />Wat had je precies willen hebben?<delay frames="30" /> <anim id="Hesitate" target="actor" targetSpec="" />Ik
