<anim id="EmotEnd" target="actor" targetSpec="" />"<tag a="50" b="19" arg="0x20 0xcd00 0x14 0x44 0x65 0x20 0x75 0x6c 0x74 0x69 0x65 0x6d 0x65 0x16 0x48 0x65 0x74 0x20 0x75 0x6c 0x74 0x69 0x65 0x6d 0x65 0x14 0x44 0x65 0x20 0x75 0x6c 0x74 0x69 0x65 0x6d 0x65" /> <tag a="50" b="23" arg="0xcd03" /><tag a="135" b="5" arg="0xcd00" />."<anim id="OhGeez" target="mainNpc" targetSpec="" /><anim id="EmotEnd" target="mainNpc" targetSpec="" /><anim id="EmotEnd" target="subNpc" targetSpec="" /></message>
