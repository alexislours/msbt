	<message label="005_a" attrib="00 00 00 00 00 00 00 00">Zeker weten!</message>
	<message label="005_b" attrib="00 00 00 00 00 00 00 00">Niet echt...</message>
	<message label="006" attrib="00 00 02 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />Heb je interesse in een van deze
