	<message label="004_a" attrib="00 00 00 00 00 00 00 00">Jazeker!</message>
	<message label="004_b" attrib="00 00 00 00 00 00 00 00">Nou, nee.</message>
	<message label="005" attrib="00 00 02 00 00 00 00 00"><anim id="QuestionMark" target="actor" targetSpec="" />Zie je iets tussen mijn spullen wat
