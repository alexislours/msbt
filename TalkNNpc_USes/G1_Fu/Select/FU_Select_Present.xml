	<message label="402" attrib="00 00 00 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />¡Rabanitos!<delay20 /><anim id="Smiling" target="actor" targetSpec="" />
¡<tag a="50" b="3" /><tag a="50" b="2" /><tag a="125" b="0" arg="0x0" />!</message>
	<message label="403" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />Muchas gracias,<delay8 /> me comeré esto
