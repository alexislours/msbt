	<message label="509" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />¿<tag a="50" b="3" /><tag a="50" b="2" /><tag a="125" b="0" arg="0x0" />?<delay20 />
<anim id="HappyFlower" target="actor" targetSpec="" />¡Qué bonito regalo!</message>
