	<message label="205" attrib="00 00 00 00 00 00 00 00"><anim id="BigSmile" target="actor" targetSpec="" />¡Para nada,<20_2 arg="3" /><delay8 /> <tag a="110" b="31" arg="0xcd01" />!<anim id="FaintSmile" target="subNpc" targetSpec="" /><delay20 />
