	<message label="001" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />Esto<tag a="110" b="0" /><delay frames="20" /> es<tag a="110" b="0" /><delay20 /><10_4 /> <anim id="HappyDance" target="actor" targetSpec="" />¡mío!</message>
