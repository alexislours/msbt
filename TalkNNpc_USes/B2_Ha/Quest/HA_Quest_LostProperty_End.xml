	<message label="006" attrib="00 00 00 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" />Esto tiene<tag a="110" b="0" /><delay frames="20" /> <anim id="HappyFlower" target="actor" targetSpec="" />¡mi guía de sentadillas!</message>
	<message label="007" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />¡Mi diario!<delay8 /> <anim id="SmugFace" target="actor" targetSpec="" />¡Yo lo llamo "Guía para
