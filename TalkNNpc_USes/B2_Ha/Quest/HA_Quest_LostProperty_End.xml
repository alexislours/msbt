	<message label="003" attrib="00 00 00 00 00 00 00 00"><anim id="HappyDance" target="actor" targetSpec="" />¡Ese es!<delay8 /> ¡Lo estaba buscando!</message>
	<message label="004" attrib="00 00 00 00 00 00 00 00"><anim id="TakenAback" target="actor" targetSpec="" />¡Ay, ese olor!<delay frames="40" /> <anim id="Blushing" target="actor" targetSpec="" />Uy, sí<tag a="110" b="0" /><delay20 />
