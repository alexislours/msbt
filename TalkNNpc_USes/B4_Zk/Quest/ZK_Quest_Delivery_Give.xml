	<message label="301" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />¿Qué me traes?</message>
	<message label="302" attrib="00 00 00 00 00 00 00 00"><anim id="IdeaBulb" target="actor" targetSpec="" />¡Ah,<delay8 /> sí!<delay20 /> Recuerdo haber comprado
