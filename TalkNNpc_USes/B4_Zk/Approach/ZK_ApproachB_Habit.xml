	<message label="004_a" attrib="00 00 00 00 00 00 00 00">Ya no tanto...</message>
	<message label="004_b" attrib="00 00 00 00 00 00 00 00">¡Aún suena bien!</message>
	<message label="005" attrib="00 00 00 00 00 00 00 00"><anim id="ShyBoy" target="actor" targetSpec="" />¡Entendido, fenómen<gender2 m="o" f="a" />!<delay8 /> <anim id="Grin" target="actor" targetSpec="" />Simplemente
