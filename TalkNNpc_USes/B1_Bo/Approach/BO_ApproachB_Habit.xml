	<message label="004_a" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />Podrías cambiarla...</message>
	<message label="004_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />¡Es perfecta!</message>
	<message label="005" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />¿En serio?<delay20 /> <anim id="EmotEnd" target="actor" targetSpec="" />Pues si eso crees,<delay8 />
