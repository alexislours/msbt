	<message label="004" attrib="00 00 02 00 00 00 00 00"><anim id="Thinking" target="actor" targetSpec="" />Mmm<tag a="110" b="0" /><delay8 /> <tag a="50" b="3" /><tag a="110" b="5" /><tag a="110" b="0" /><delay20 />
