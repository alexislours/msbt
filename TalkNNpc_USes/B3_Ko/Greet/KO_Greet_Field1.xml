	<message label="002" attrib="00 00 0c 01 00 03 00 00"><anim id="Smiling" target="actor" targetSpec="" />¡Hola!<delay20 /> <anim id="Hello" target="actor" targetSpec="" />¿Cómo estás?<setNpcTalkChoices chatOption="0" cancelOption="0" /></message>
	<message label="003" attrib="00 00 0c 01 00 04 00 00"><anim id="Greeting" target="actor" targetSpec="" />¡Tienes buena pinta hoy!<setNpcTalkChoices chatOption="0" cancelOption="1" /></message>
	<message label="004" attrib="00 00 0c 01 00 09 00 00"><anim id="Greeting" target="actor" targetSpec="" />¡La noche es joven!
<colour id="4" />(Bueno, a mi edad no tanto<tag a="110" b="0" />).<colour id="65535" /><setNpcTalkChoices chatOption="1" cancelOption="1" /></message>
	<message label="011" attrib="00 00 0c 01 00 00 00 07"><anim id="Smiling" target="actor" targetSpec="" />Qué,<delay8 /> ¿cómo te trata la vida?<setNpcTalkChoices chatOption="0" cancelOption="1" /></message>
	<message label="012" attrib="00 00 0c 01 00 00 00 07"><anim id="FaintSmile" target="actor" targetSpec="" />Verte tan ocupad<gender2 m="o" f="a" /> me causa cierto
desasosiego.<setNpcTalkChoices chatOption="0" cancelOption="1" /></message>
	<message label="013" attrib="00 00 0c 01 00 00 00 07"><anim id="FaintSmile" target="actor" targetSpec="" />Anda,<delay8 /> ¡si eres tú!<setNpcTalkChoices chatOption="0" cancelOption="1" /></message>
	<message label="021" attrib="00 00 0c 01 00 00 00 08"><anim id="Shocked" target="actor" targetSpec="" />Vaya,<delay8 /> <anim id="Smiling" target="actor" targetSpec="" />¡pero si es mi <gender2 m="estimado" f="estimada" />
<tag a="110" b="3" />!<delay20 /> Me alegro de verte,<delay8 /> <tag a="110" b="5" />.<setNpcTalkChoices chatOption="1" cancelOption="1" /></message>
	<message label="022" attrib="00 00 0c 01 00 00 00 08"><anim id="Greeting" target="actor" targetSpec="" />¡Hola,<delay8 /> <tag a="110" b="3" />!
<anim id="FaintSmile" target="actor" targetSpec="" />Qué curioso<delay8 /> encontrarte por aquí.<setNpcTalkChoices chatOption="1" cancelOption="1" /></message>
	<message label="023" attrib="00 00 0c 01 00 00 00 08"><anim id="Greeting" target="actor" targetSpec="" />¡Me alegro de verte,<delay8 /> <tag a="110" b="3" />!<setNpcTalkChoices chatOption="1" cancelOption="1" /></message>
	<message label="031" attrib="00 00 0c 01 00 00 00 09"><anim id="Greeting" target="actor" targetSpec="" />¡<tag a="50" b="3" /><tag a="110" b="14" />Hola, muchach<gender2 m="o" f="a" /><endTag a="110" b="14" />!
<anim id="Smiling" target="actor" targetSpec="" />¡Qué ganas tenía de verte,<delay8 /> <tag a="110" b="3" />!<setNpcTalkChoices chatOption="1" cancelOption="1" /></message>
	<message label="032" attrib="00 00 0c 01 00 00 00 09"><anim id="Smiling" target="actor" targetSpec="" />¡<tag a="50" b="3" /><tag a="110" b="14" />Pero a quién tenemos aquí<endTag a="110" b="14" />!<delay20 />
¡Si es <tag a="110" b="3" />!<delay20 /> <anim id="FaintSmile" target="actor" targetSpec="" />Siempre
