<msbt attribsize="8">
	<message label="001" attrib="00 00 02 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />¿Ya te vas?</message>
	<message label="001_a" attrib="00 00 00 00 00 00 00 00">Pues sí</message>
	<message label="001_b" attrib="00 00 00 00 00 00 00 00">Pues no</message>
	<message label="002" attrib="00 00 02 00 00 00 00 00"><anim id="QuestionMark" target="actor" targetSpec="" />¿Vas a salir?</message>
	<message label="002_a" attrib="00 00 00 00 00 00 00 00">¿Sí?</message>
	<message label="002_b" attrib="00 00 00 00 00 00 00 00">¡Claro que no!</message>
	<message label="003" attrib="00 00 02 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />¿Vas a algún lado, <tag a="110" b="3" />?</message>
	<message label="003_a" attrib="00 00 00 00 00 00 00 00">Sí</message>
	<message label="003_b" attrib="00 00 00 00 00 00 00 00">No</message>
	<message label="004" attrib="00 00 00 00 00 00 00 00"><anim id="Hello" target="actor" targetSpec="" />¡Esto dímelo antes!<delay8 />
<anim id="Nodding" target="actor" targetSpec="" />Regreso a mi casa, entonces.<delay20 />
<anim id="Smiling" target="actor" targetSpec="" />¡Ya nos vemos otro día!</message>
	<message label="005" attrib="00 00 00 00 00 00 00 00"><anim id="Restless" target="actor" targetSpec="" />Mmm<tag a="110" b="0" /><delay8 /> <anim id="Greeting" target="actor" targetSpec="" />¡Pues yo también me voy!<delay20 />
<anim id="BigSmile" target="actor" targetSpec="" />¡Gracias mil por haberme invitado!</message>
	<message label="006" attrib="00 00 00 00 00 00 00 00"><anim id="ShyBoy" target="actor" targetSpec="" />Bueno<tag a="110" b="0" /><delay8 /> Pues yo también me pongo
en marcha.<delay20 /> <anim id="Smiling" target="actor" targetSpec="" />¡Gracias por haberme
invitado a ver cómo tienes la casa!</message>
	<message label="007" attrib="00 00 00 00 00 00 00 00"><anim id="ShyBoy" target="actor" targetSpec="" />Buf<tag a="110" b="0" /><delay8 /> <anim id="OhGeez" target="actor" targetSpec="" />ya pensaba que me ibas a dejar
aquí sola, guapi<tag a="110" b="0" /></message>
	<message label="008" attrib="00 00 00 00 00 00 00 00"><anim id="Laughing" target="actor" targetSpec="" />¡Qué SUSTO, amigui!<delay8 /> 
<anim id="Blushing" target="actor" targetSpec="" />Ya pensaba que ibas a dejarme
