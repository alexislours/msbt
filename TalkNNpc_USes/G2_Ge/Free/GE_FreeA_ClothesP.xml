	<message label="401" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="401_01" attrib="00 00 00 00 00 00 00 00"><anim id="FaintSmile" target="actor" targetSpec="" />Sé que tú me entiendes:<delay20 /> cuando
