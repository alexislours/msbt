	<message label="002_02" attrib="00 00 00 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />¡Oooh! ¡<tag a="50" b="3" /><tag a="50" b="2" /><tag a="125" b="0" arg="0x0" />!</message>
	<message label="002_03" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />¡Vaya! Esto no me lo esperaba.<delay20 />
<anim id="Smiling" target="actor" targetSpec="" />¡<tag a="50" b="3" /><tag a="50" b="2" /><tag a="125" b="0" arg="0x0" />!</message>
	<message label="003" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="003_01" attrib="00 00 00 00 00 00 00 00"><anim id="Restless" target="actor" targetSpec="" />¿Eh? ¿Para mí?</message>
	<message label="003_02" attrib="00 00 00 00 00 00 00 00"><anim id="HappyDance" target="actor" targetSpec="" />¿Me compraste algo? ¡Eres la onda!</message>
	<message label="003_03" attrib="00 00 00 00 00 00 00 00"><anim id="Hello" target="actor" targetSpec="" />¡Vaya! ¿Qué será?</message>
	<message label="004" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="004_01" attrib="00 00 00 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />¿Lo envolviste y todo?<delay20 />
<anim id="Smiling" target="actor" targetSpec="" />¡Qué misterioso! <delay8 />¡Voy a abrirlo YA!</message>
	<message label="004_02" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />¡Ay! ¿Qué será?<delay20 /> ¡Me muero por
