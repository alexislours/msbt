	<message label="009" attrib="00 00 00 00 00 00 00 00"><20_3 arg="5" /><anim id="Angry" target="actor" targetSpec="" />Vraiment,<delay8 /> <20_2 arg="1" /><tag a="110" b="31" arg="0xcd01" />?<delay20 />
