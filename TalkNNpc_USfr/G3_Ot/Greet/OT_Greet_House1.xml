	<message label="036" attrib="00 00 0c 01 00 07 00 09"><anim id="Greeting" target="actor" targetSpec="" /><tag a="50" b="3" /><tag a="110" b="14" />Bonjour, <tag a="110" b="3" /><endTag a="110" b="14" />!<delay40 />
