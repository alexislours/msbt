	<message label="034" attrib="00 00 0c 01 00 00 00 09"><anim id="Nodding" target="actor" targetSpec="" /><tag a="50" b="3" /><tag a="110" b="14" />Allo, <tag a="110" b="3" /><endTag a="110" b="14" />!
