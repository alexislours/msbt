	<message label="102_b" attrib="68 00 00 00 00 00 00 00">J'essaie...</message>
	<message label="102_c" attrib="69 00 00 00 00 00 00 00">Bof...</message>
	<message label="103" attrib="00 00 00 00 00 00 00 00"><anim id="BigSmile" target="actor" targetSpec="" />Quelle bonne nouvelle!<delay frames="40" /> <anim id="Explain" target="actor" targetSpec="" />Je t'avoue
