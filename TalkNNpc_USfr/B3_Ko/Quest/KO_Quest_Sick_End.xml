	<message label="200" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />Tu m'as sauvé la mise!<delay frames="30" /> <anim id="EmotEnd" target="actor" targetSpec="" />Mais ta récompense va devoir attendre,<delay8 /><anim id="Grin" target="actor" targetSpec="" />
