	<message label="106" attrib="00 00 00 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" />Avec emballage?<delay20 /><anim id="FaintSmile" target="actor" targetSpec="" /> <tag a="115" b="1" arg="0xcd01" />
