	<message label="011" attrib="00 00 00 00 00 00 00 00"><anim id="Scheming" target="actor" targetSpec="" />Tu l'auras voulu. <delay8 />V'là ton sort!<delay frames="40" />
<anim id="Cheering" target="actor" targetSpec="" /><delay20 /><10_10 /><size value="200" />BOUUU<delay8 />-YAAA!<size value="100" /><10_7 arg="180" /></message>
	<message label="012" attrib="00 00 00 00 00 00 00 00"><anim id="Laughing" target="actor" targetSpec="" />Hu hu hu! <delay20 />Tu devrais te voir!<delay20 /> <anim id="Aha" target="actor" targetSpec="" /><delay20 />C'est
