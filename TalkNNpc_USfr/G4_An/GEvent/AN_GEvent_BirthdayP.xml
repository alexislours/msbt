	<message label="023_a" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="player" targetSpec="" />Tiens, prends ça!</message>
	<message label="023_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="player" targetSpec="" />Merci!</message>
	<message label="025" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />Dis,<delay8 /> c'était pas ta fête hier?<delay frames="40" />
