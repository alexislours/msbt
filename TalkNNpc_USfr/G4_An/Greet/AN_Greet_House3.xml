<msbt attribsize="8">
	<message label="001" attrib="00 00 0c 01 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />J'savais qu'tu reviendrais. <delay20 /><anim id="WrySmile" target="actor" targetSpec="" />Ma piaule
