	<message label="103" attrib="00 00 00 00 00 00 00 00"><anim id="BigSmile" target="actor" targetSpec="" />C'est <tag a="50" b="2" /><tag a="125" b="4" arg="0x0" />!<delay frames="40" /><anim id="Hesitate" target="actor" targetSpec="" /> Je me suis dit que ça te plairait<tag a="110" b="0" /></message>
