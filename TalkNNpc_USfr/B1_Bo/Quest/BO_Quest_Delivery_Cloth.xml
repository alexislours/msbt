	<message label="007" attrib="00 00 00 00 00 00 00 00">Hmm<tag a="110" b="0" /><delay frames="40" /> Oui<tag a="110" b="0" /> <anim id="Grin" target="actor" targetSpec="" />Je l'essaierai plus
