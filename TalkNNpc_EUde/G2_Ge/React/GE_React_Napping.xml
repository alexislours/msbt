	<message label="010" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="010_01" attrib="00 00 00 00 00 00 00 00"><anim id="Blushing" target="actor" targetSpec="" />Ich bin gerade erst aufgewacht, <delay8 />aber
