	<message label="005" attrib="00 00 00 00 00 00 00 00"><20_2 arg="10" /><20_2 arg="9" /><anim id="Thinking" target="actor" targetSpec="" /><tag a="50" b="17" arg="0x12 0xcd00 0xa 0x45 0x69 0x6e 0x65 0x6e 0x8 0x45 0x69 0x6e 0x65 0x6 0x45 0x69 0x6e" /> <tag a="50" b="23" arg="0xcd01" /><tag a="125" b="0" arg="0x0" />
