	<message label="403_a" attrib="00 00 00 00 00 00 00 00">Klingt gut!</message>
	<message label="403_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />Das kannst du besser.</message>
