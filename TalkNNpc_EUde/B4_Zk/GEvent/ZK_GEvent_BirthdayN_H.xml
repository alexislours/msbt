<msbt attribsize="8">
	<message label="001" attrib="00 00 00 00 00 00 00 00"><anim id="Grin" target="actor" targetSpec="" />Willkommen zur Party!<delay20 />
<anim id="Dance" target="actor" targetSpec="" />Ich habe Geburtstag!</message>
