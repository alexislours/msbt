	<message label="018_01" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />So <tag a="50" b="17" arg="0x17 0xcd00 0x4 0x27 0x6e 0x6 0x27 0x6e 0x65 0x4 0x27 0x6e" /> <tag a="125" b="5" arg="0xcd00" />
