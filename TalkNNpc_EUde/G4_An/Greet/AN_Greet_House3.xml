	<message label="003" attrib="00 00 0c 01 00 00 00 00"><anim id="Grin" target="actor" targetSpec="" />Willkommen daheim!<delay frames="40" /> <anim id="Laughing" target="actor" targetSpec="" />Muhaha!<delay20 /> Ich
