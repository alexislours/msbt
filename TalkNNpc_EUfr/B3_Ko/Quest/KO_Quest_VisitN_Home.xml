	<message label="201_a" attrib="00 00 00 00 00 00 00 00">Avec plaisir !</message>
	<message label="201_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />Sinon plus tard ?</message>
	<message label="202" attrib="00 00 02 00 00 00 00 00"><anim id="FaintSmile" target="actor" targetSpec="" />Hé,<delay8 /> <tag a="110" b="3" />. <delay20 /><anim id="SmugFace" target="actor" targetSpec="" />Tu te sens d'humeur compétitive ?<delay20 /> Ça te dirait de jouer à un jeu ?</message>
	<message label="202_a" attrib="00 00 00 00 00 00 00 00">Oh que oui !</message>
