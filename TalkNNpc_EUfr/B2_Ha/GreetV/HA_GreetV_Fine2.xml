	<message label="006" attrib="00 00 0c 01 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" />Yo, <tag a="110" b="3" /> !<delay20 /> <anim id="Smiling" target="actor" targetSpec="" />On se croise à nouveau, <tag a="110" b="6" />.<setNpcTalkChoices chatOption="0" cancelOption="0" /></message>
