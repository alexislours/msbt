<msbt attribsize="8">
	<message label="001" attrib="00 00 0c 01 00 07 00 00"><anim id="Greeting" target="actor" targetSpec="" />Bonjour !<delay40 /> <anim id="Sleepy" target="actor" targetSpec="" />J'ai ouvert les yeux plus
