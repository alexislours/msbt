	<message label="003_b" attrib="00 00 00 00 00 00 00 00">下次再說吧</message>
	<message label="004" attrib="00 00 00 00 00 00 00 00"><anim id="Worried" target="actor" targetSpec="" />哎呀，這樣啊<tag a="110" b="0" /><delay20 />
