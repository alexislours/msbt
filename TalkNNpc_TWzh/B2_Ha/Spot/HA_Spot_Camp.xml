	<message label="202" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="202_01" attrib="00 00 00 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" />嗨！<delay8 />我是<tag a="110" b="10" />！<delay frames="30" />
