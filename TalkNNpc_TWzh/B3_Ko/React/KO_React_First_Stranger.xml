	<message label="511" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="511_01" attrib="00 00 00 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" />哦！<delay20 />
