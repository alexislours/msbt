	<message label="002" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="002_01" attrib="00 00 00 00 00 00 00 00"><anim id="Laughing" target="actor" targetSpec="" />啊哈哈，<delay20 />嚇到你了嗎？<delay frames="40" />
