	<message label="007" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />Da parte di <tag a="115" b="1" arg="0xcd01" />?</message>
	<message label="101" attrib="00 00 00 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />Ma guarda<tag a="110" b="0" /> <delay20 /><anim id="HappyFlower" target="actor" targetSpec="" />Lo ha perfino incartato
