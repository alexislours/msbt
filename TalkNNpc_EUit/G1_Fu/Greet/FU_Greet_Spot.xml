<msbt attribsize="8">
	<message label="001" attrib="00 00 0c 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />Guarda chi si vede, <delay20 /><tag a="110" b="3" />!<delay frames="40" /> <anim id="FaintSmile" target="actor" targetSpec="" />Che piacevole coincidenza,<delay20 /> <tag a="110" b="5" />.<setNpcTalkChoices chatOption="0" cancelOption="1" /></message>
	<message label="002" attrib="00 00 0c 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" />Salut, <delay8 /><tag a="110" b="3" />!<delay20 /> <anim id="WrySmile" target="actor" targetSpec="" />Sei qui per fare un saluto agli amici insetti, <tag a="110" b="6" />?<setNpcTalkChoices chatOption="0" cancelOption="0" /></message>
	<message label="003" attrib="00 00 0c 00 00 00 00 00"><anim id="Laughing" target="actor" targetSpec="" />Vuoi fare un tuffo? <delay20 /><anim id="WrySmile" target="actor" targetSpec="" />Io non dirò
nulla.<delay8 /> Sarò muta come un poisson,
<tag a="110" b="6" />!<setNpcTalkChoices chatOption="1" cancelOption="0" /></message>
	<message label="004" attrib="00 00 0c 00 00 00 00 00"><anim id="TakenAback" target="actor" targetSpec="" />Oh, <tag a="110" b="3" />!<pageBreak /><anim id="WrySmile" target="actor" targetSpec="" />Non mi aspettavo di incontrare
