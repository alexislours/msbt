	<message label="155" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="155_01" attrib="00 00 00 00 00 00 00 00"><anim id="Surprised" target="actor" targetSpec="" />Eh?! <delay20 /><anim id="Blushing" target="actor" targetSpec="" />Davvero mi hai preso
