	<message label="001" attrib="00 00 0c 00 00 00 00 00"><anim id="Aha" target="actor" targetSpec="" />Ehi, <tag a="110" b="3" />!<delay frames="40" /> <anim id="FaintSmile" target="actor" targetSpec="" />Anche tu qui per farti una cultura, <tag a="110" b="6" />?<setNpcTalkChoices chatOption="0" cancelOption="0" /></message>
