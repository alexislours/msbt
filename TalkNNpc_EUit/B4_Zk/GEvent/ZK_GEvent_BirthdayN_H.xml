	<message label="006_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="player" targetSpec="" />Io non ho niente...</message>
	<message label="007" attrib="00 00 00 00 00 00 00 00"><anim id="Worried" target="actor" targetSpec="" />Oh, beh<tag a="110" b="0" /> Non importa. <delay20 /><anim id="IdeaBulb" target="actor" targetSpec="" />Puoi farmi
