<msbt attribsize="8">
	<message label="001" attrib="00 00 0c 01 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />Oh, <delay8 />di nuovo tu!<delay8 /> <anim id="EmotEnd" target="actor" targetSpec="" />Che succede?<setNpcTalkChoices chatOption="0" cancelOption="0" /></message>
