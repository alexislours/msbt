	<message label="404_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />Scendi ancora.</message>
	<message label="405" attrib="00 00 02 00 00 00 00 00"><anim id="Frantic" target="actor" targetSpec="" />Ma stai scherzando?!<delay frames="60" /><anim id="Silent" target="actor" targetSpec="" /> Va bene, tieniti
