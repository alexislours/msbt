	<message label="207_a" attrib="00 00 00 00 00 00 00 00">Affare fatto!</message>
	<message label="207_b" attrib="00 00 00 00 00 00 00 00">Forse non mi serve...</message>
	<message label="208" attrib="00 00 00 00 00 00 00 00"><anim id="HappyDance" target="actor" targetSpec="" />Evviva! Finalmente ce l'ho fatta!</message>
	<message label="209" attrib="00 00 00 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />Ma mi sa che<tag a="110" b="0" /><delay frames="30" /><anim id="WrySmile" target="actor" targetSpec="" /> mi sono lasciato
