	<message label="003_a" attrib="00 00 00 00 00 00 00 00">Sì, cambialo!</message>
	<message label="003_b" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />Sii te stessa! Tienilo!</message>
	<message label="004" attrib="00 00 02 00 00 00 00 00"><anim id="Greeting" target="actor" targetSpec="" />Allucinante, <tag a="110" b="3" />! 
