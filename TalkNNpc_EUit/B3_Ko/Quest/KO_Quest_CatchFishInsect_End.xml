	<message label="031" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />Grazie mille!</message>
	<message label="032" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />Mi sei stat<gender2 m="o" f="a" /> di grande aiuto.</message>
	<message label="033" attrib="00 00 00 00 00 00 00 00"><anim id="Smiling" target="actor" targetSpec="" />Grazie mille!<delay20 /> Ho fatto troppo bene
