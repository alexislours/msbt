	<message label="002_06_b" attrib="00 00 00 00"><anim id="Explain" target="actor" targetSpec="" />Cambiare l'inno.</message>
	<message label="002_06_c" attrib="00 00 00 00"><anim id="Explain" target="actor" targetSpec="" />Cambiare la bandiera.</message>
